{"story":{"group_1":[{"autoTurnLast":0.01,"bg":"bg_adv_21201.jpg","bgm":"bgm01_anime02","chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100101,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":100201,"motion":0,"pos":2}],"turnChangeIn":"fadeIn"},{"chara":[{"id":100201,"motion":301}],"nameLeft":"Iroha","nameRight":"Yachiyo","textRight":"Well then, sorry about this,@but please take care of the house.","turnChangeOut":"fadeOut"},{"chara":[{"id":100101,"motion":200}],"textLeft":"Don't worry about it.@We're all here, it's fine."},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":100201},{"effect":"fadeout","id":100101}],"textClear":1},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100501,"motion":100,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":100401,"motion":1,"pos":1},{"cheek":0,"face":"mtn_ex_010.exp.json","id":101501,"motion":200,"pos":2}]},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":100501},{"effect":"fadeout","id":100401},{"effect":"fadeout","id":101501}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100101,"motion":100,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":100201,"motion":0,"pos":2}],"nameLeft":"Iroha","nameRight":"Yachiyo","textLeft":"Take care, Yachiyo!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100201,"motion":100}],"textRight":"I'm off."},{"chara":[{"cheek":0,"face":"mtn_ex_051.exp.json","id":100201,"lipSynch":0}],"textRight":"............@[wait:1.5][chara:100201:cheek_0][chara:100201:face_mtn_ex_014.exp.json]............"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100101,"motion":0}],"textLeft":"Yachiyo?"},{"chara":[{"eyeClose":1,"id":100201,"motion":300}],"textRight":"...No, it's nothing."},{"chara":[{"eyeClose":0,"id":100201,"motion":100}],"textRight":"...I'm off."},{"autoTurnLast":0.5,"chara":[{"effect":"fadeout","id":100201}],"textClear":1},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":100101}],"textClear":1},{