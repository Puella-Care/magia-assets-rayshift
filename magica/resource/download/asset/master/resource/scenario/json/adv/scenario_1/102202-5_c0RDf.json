a":[{"cheek":2,"face":"mtn_ex_030.exp.json","id":101501,"motion":0,"pos":0,"tear":1,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-14","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-13"}],"nameLeft":"Ui","textLeft":"Okay..."},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":101501,"voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-14"},{"effect":"fadeout","id":100401}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"motion":200,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-15"},{"cheek":0,"face":"mtn_ex_040.exp.json","id":100301,"motion":0,"pos":2}],"nameLeft":"Iroha","textLeft":"Tsuruno, is it okay if I tweak this a little?"},{"chara":[{"id":100101,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-16","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-15"}],"textLeft":"I can probably make it a little milder@with some vinegar or eggs."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100301,"motion":300,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-17","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-16"}],"nameRight":"Tsuruno","textRight":"Ah, sure...sorry..."},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":100101},{"effect":"fadeout","id":100301,"voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-17"}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_021.exp.json","id":100201,"motion":400,"pos":1,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-18"}],"nameCenter":"Yachiyo","textCenter":"Wha...!"},{"chara":[{"id":100201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-19","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-18"}],"textCenter":"Hold on, what is this smell?@I feel like I'm choking on it..."},{"autoTurnLast":0.2,"chara":[{"id":100201,"pos":2,"voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-19"}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100301,"motion":200,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-20"}],"nameLeft":"Tsuruno","textLeft":"Urgh, what timing...!"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100201,"motion":200,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-21","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-20"}],"nameRight":"Yachiyo","textRight":"Please open the window...@It's like the room is made out of chilli peppers..."},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":100301,"motion":101,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-22","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-21"}],"textLeft":"R-right away!"},{"chara":[{"cheek":0,"face":"mtn_ex_021.exp.json","id":100201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-23","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-22"}],"textRight":"Tsuruno."},{"chara":[{"id":100301,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-24","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-23"}],"textLeft":"Yes!"},{"chara":[{"cheek":0,"face":"mtn_ex_042.exp.json","id":100201,"motion":201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-25","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-24"}],"textRight":"You know the state of our finances, right...?"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100301,"motion":400,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-26","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-25"}],"textLeft":"Eeek...!"},{"autoTurnLast":0.5,"bg":"bg_adv_21263.jpg","bgm":"stop","chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100101,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":100201,"motion":0,"pos":2}],"turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-26"},{"bgm":"bgm03_story02","chara":[{"id":100101,"motion":100,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-27"}],"nameLeft":"Iroha","textLeft":"Welcome home, Yachiyo.","turnChangeOut":"fadeOut"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-28","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-27"}],"textLeft":"Didn't we have a stock of vinegar?"},{"chara":[{"id":100201,"motion":200,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-29","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-28"}],"nameRight":"Yachiyo","textRight":"It should be under the sink."},{"chara":[{"id":100201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-30","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-29"}],"textRight":"If you're adjusting the flavor,@I'll help you with that."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100101,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-31","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-30"}],"textLeft":"Thank you."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100201,"motion":100,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-32","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-31"}],"textRight":"Are you okay, Sana?"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100401,"motion":100,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-33","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-32"}],"nameLeft":"Sana","textLeft":"Yes..."},{"chara":[{"id":100401,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-34","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-33"}],"textLeft":"We had some cold rice and eggs, so I'm@making something for Ui with that."},{"chara":[{"cheek":0,"face":"mtn_ex_014.exp.json","id":100201,"motion":301,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-35","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-34"}],"textRight":"You've gotten a lot better at cooking since@you came here, haven't you?"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100401,"motion":200,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-36","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-35"}],"textLeft":"[chara:100401:effect_emotion_joy_0][se:7222_happy]Yes..."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100201,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-37","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-36"}],"textRight":"Iroha."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100101,"motion":0,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-38","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-37"}],"nameLeft":"Iroha","textLeft":"Yes?"},{"chara":[{"id":100201,"motion":201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-39","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-38"}],"textRight":"Could you go to Touka's place tomorrow?"},{"chara":[{"id":100201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-40","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-39"}],"textRight":"They said they want a proper look at the Bracelet."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"motion":301,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-41","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-40"}],"textLeft":"It's better to have them look at it in person, right?"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100201,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-42","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-41"}],"textRight":"Yeah, that seems to be their thought as well."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100101,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-43","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-42"}],"textLeft":"Ah..."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100201,"motion":100,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-44","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-43"}],"textRight":"Hm? Did you have plans already?"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"motion":200,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-45","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-44"}],"textLeft":"Yes... I was going to meet@with some people tomorrow."},{"chara":[{"id":100201,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-46","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-45"}],"textRight":"With who? Kanagi?"},{"chara":[{"id":100101,"motion":1,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-47","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-46"}],"textLeft":"No, with some girls we met@at the supermarket today."},{"chara":[{"id":100101,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-48","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-47"}],"textLeft":"They came to Kamihama from the countryside@and don't know their way around."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100101,"motion":301,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-49","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-48"}],"textLeft":"So Ui and I promised to help them out tomorrow..."},{"chara":[{"cheek":0,"face":"mtn_ex_051.exp.json","id":100201,"motion":200,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-50","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-49"}],"textRight":"Well, that's a bit sudden."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":101501,"motion":200,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-51","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-50"}],"nameLeft":"Ui","textLeft":"It's fine, Sis, I'll just go by myself!"},{"chara":[{"cheek":0,"face":"mtn_ex_042.exp.json","id":100201,"motion":100,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-52","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-51"}],"textRight":"Absolutely not."},{"chara":[{"id":100201,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-53","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-52"}],"textRight":"I won't allow you to go off and do that alone."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":101501,"motion":400,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-54","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-53"}],"textLeft":"Why not...?!"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100201,"motion":301,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-55","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-54"}],"textRight":"An elementary schooler meeting@up with strangers on her own..."},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":100401,"motion":200,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-56","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-55"}],"nameLeft":"Sana","textLeft":"Um, then, I'll go with her...!"},{"chara":[{"cheek":0,"face":"mtn_ex_040.exp.json","id":100401,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-57","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-56"}],"textLeft":"Though I don't think they'll be able to see me..."},{"chara":[{"id":100401,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-58","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-57"}],"textLeft":"I do think I can support her a little..."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":101501,"motion":100,"pos":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-59","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-58"}],"nameLeft":"Ui","textLeft":"See! Please, Yachiyo!"},{"chara":[{"eyeClose":1,"id":100201,"lipSynch":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-60","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-59"}],"textRight":"..."},{"chara":[{"eyeClose":0,"id":100201,"lipSynch":1,"motion":0,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-61","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-60"}],"textRight":"Fine."},{"chara":[{"id":100201,"motion":100,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-62","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-61"}],"textRight":"I think it'll be alright, but don't@cause them trouble, okay?"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":101501,"motion":200,"voiceFull":"fullvoice/section_102202/vo_full_102202-5-63","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-62"}],"textLeft":"Yeah!"},{"autoTurnLast":2.0,"bgm":"stop","turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_102202/vo_full_102202-5-63"}]},"version":3}
