uto":"fullvoice/section_102505/vo_full_102505-12-69"},{"narration":"we will eventually be forced to take lives ourselves...","voiceFullAuto":"fullvoice/section_102505/vo_full_102505-12-70"},{"autoTurnLast":2.0,"bgm":"stop","item":[{"path":"resource/scenario/img/part2_chapter4/102505_pray_rabi.jpg","visible":"hide"}],"narrationEffect":"out","turnChangeIn":"fadeIn"}]},"version":3}
