utoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800700,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801000,"motion":0,"pos":2}]},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":800700},{"effect":"fadeout","id":801000}]},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800400,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800500,"motion":0,"pos":2}]},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":800400},{"effect":"fadeout","id":800500}]},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800900,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801200,"motion":0,"pos":1},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800800,"motion":0,"pos":2}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":800900},{"effect":"fadeout","id":801200},{"effect":"fadeout","id":800800}]},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801500,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801400,"motion":0,"pos":2}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":801500},{"effect":"fadeout","id":801400}]},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800300,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800102,"motion":0,"pos":2}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":800300},{"effect":"fadeout","id":800102}]},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":800202,"motion":0,"pos":1}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":800202}]},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":301601,"motion":100,"pos":2},{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":0,"pos":0}],"nameRight":"Kokoro","textRight":"That sure is a lot of people.@There's someone alone, too...?"},{"chara":[{"id":302901,"motion":200,"pos":0}],"nameLeft":"Masara","textLeft":"I heard something called a \"solo wedding\" exists."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"motion":1,"pos":2}],"nameRight":"Kokoro","textRight":"Heeh, I didn't know that!@That's kind of nice!"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":302901,"motion":0,"pos":0}],"nameLeft":"Masara","textLeft":"Oh...over there..."},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":302901},{"effect":"fadeout","id":301601}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"autoTurnLast":2.5,"chara":[{"cheek":0,"face":"mtn_ex_001.exp3.json","id":302601,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_020.exp3.json","id":302701,"motion":1,"pos":1},{"cheek":0,"face":"mtn_ex_030.exp3.json","id":302801,"motion":0,"pos":2}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":302601},{"effect":"fadeout","id":302701},{"effect":"fadeout","id":302801}]},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"motion":400,"pos":2},{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":0,"pos":0}],"nameRight":"Kokoro","textRight":"Ayame and the Azaleas!"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":301601},{"effect":"fadeout","id":302901}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":301601,"motion":1,"pos":2},{"cheek":0,"face":"mtn_ex_050.exp3.json","id":302801,"motion":0,"pos":0}],"nameRight":"Kokoro","textRight":"Hey there, Ayame!"},{"chara":[{"id":302801,"motion":400,"pos":0}],"nameLeft":"Ayame","textLeft":"It's Kokoro!@[chara:302801:lipSynch_0][wait:0.8][chara:302801:cheek_0][chara:302801:face_mtn_ex_030.exp3.json][chara:302801:lipSynch_1]You've gotta help me out."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":301601,"motion":200,"pos":2}],"nameRight":"Kokoro","textRight":"What's wrong?"},{"chara":[{"id":302801,"motion":0,"pos":0}],"nameLeft":"Ayame","textLeft":"We're chaking family photos,@so we gotta pick outfits, but―"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":301601},{"effect":"fadeout","id":302801}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp3.json","id":302701,"motion":100,"pos":2},{"cheek":0,"face":"mtn_ex_011.exp3.json","id":302601,"motion":0,"pos":0}],"nameRight":"Hazuki","textRight":"Ayame would look better in something cute,@wouldn't she?"},{"chara":[{"id":302601,"motion":100,"pos":0}],"nameLeft":"Konoha","textLeft":"It's a little expensive,"},{"chara":[{"id":302601,"pos":0}],"nameLeft":"Konoha","textLeft":"but if it's for Ayame, price isn't a factor."},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":302601},{"effect":"fadeout","id":302701}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":302801,"motion":300,"pos":0},{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":0,"pos":2}],"nameLeft":"Ayame","textLeft":"They're fussin' over me so much,@but I'd be fine with anything."},{"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":400,"pos":2}],"nameRight":"Masara","textRight":"...Well, there's no helping it."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":302801,"motion":0,"pos":0}],"nameLeft":"Ayame","textLeft":"There isn't?"},{"chara":[{"id":302901,"motion":200,"pos":2}],"nameRight":"Masara","textRight":"Those two cannot help but find you adorable."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":302801,"motion":100,"pos":0}],"nameLeft":"Ayame","textLeft":"That sooo...@Well, no helpin' it then!"},{"autoTurnLast":0.2,"chara":[{"id":302901,"pos":0}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":301601,"lipSynch":0,"motion":0,"pos":2}],"nameRight":"Kokoro","textRight":"..."},{"autoTurnLast":1.0,"bgm":"stop","chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"motion":0,"pos":2}],"turnChangeIn":"fadeIn"},{"bgm":"bgm03_story02","chara":[{"id":301601,"motion":400,"pos":2}],"nameRight":"Kokoro","textRight":"Hearing you say something like@that surprised me a little.","turnChangeOut":"fadeOut"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":302901,"motion":100,"pos":0}],"nameLeft":"Masara","textLeft":"It did?"},{"chara":[{"id":301601,"motion":100,"pos":2}],"nameRight":"Kokoro","textRight":"Because you're always saying that@things like that are illogical, aren't you?"},{"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp3.json","id":302901,"pos":0}],"nameLeft":"Masara","textLeft":"...Some emotions surpass logic.@You taught me that, didn't you?"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":301601,"motion":0,"pos":2}],"nameRight":"Kokoro","textRight":"Huh?"},{"chara":[{"id":301601,"pos":2},{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp3.json","id":302901}],"nameRight":"Kokoro","textRight":"...Mm? Those people..."},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":302901},{"effect":"fadeout","id":301601}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"autoTurnLast":2.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801100,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801000,"motion":0,"pos":2}]},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":801100},{"effect":"fadeout","id":801000}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"motion":400,"pos":2},{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":0,"pos":0}],"nameRight":"Kokoro","textRight":"I met them at the beach...!"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":302901},{"effect":"fadeout","id":301601}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801100,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":801000,"motion":0,"pos":2}],"nameLeft":"","textLeft":"I decided to take up cooking classes,"},{"chara":[{"id":801100,"pos":0}],"nameLeft":"","textLeft":"so that when you come back, I'll be able to@serve up some delicious meals for you."},{"chara":[{"id":801000,"pos":2}],"nameRight":"","textRight":"Then I'll just have to learn some foreign cooking@and make you something too, won't I?"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":801100},{"effect":"fadeout","id":801000}],"textClear":1},{"autoTurnLast":0.1,"bg":"bg_adv_20255.jpg","bgChangeEffect":"cameraPan"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":302901,"motion":1,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"motion":1,"pos":2}],"nameLeft":"Masara","textLeft":"...You've met them, Kokoro?"},{"chara":[{"id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"I picked up their handkerchief when@they dropped it at the beach before."},{"chara":[{"id":301601,"motion":300,"pos":2}],"nameRight":"Kokoro","textRight":"I happened to overhear them talking back then,"},{"chara":[{"id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"worrying over stuff like the decision@to get married and live separately."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"But... *Giggle*@I'm glad, they seem happy."},{"chara":[{"id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"...After all, happiness comes in many forms."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":302901,"motion":400,"pos":0}],"nameLeft":"Masara","textLeft":"...Yes, it does."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"motion":400,"pos":2}],"nameRight":"Kokoro","textRight":"That's right, I think I can imagine what it feels@like for two people to get married now!"},{"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":302901,"motion":0,"pos":0}],"nameLeft":"Masara","textLeft":"As the models for photos, you mean...right?"},{"chara":[{"id":301601,"motion":0,"pos":2}],"nameRight":"Kokoro","textRight":"Yeah!"},{"chara":[{"id":302901,"motion":300,"pos":0}],"nameLeft":"Masara","textLeft":"...I still don't quite understand@the feeling of getting married."},{"chara":[{"id":302901,"pos":0}],"nameLeft":"Masara","textLeft":"I have had thoughts, though,"},{"chara":[{"id":302901,"pos":0}],"nameLeft":"Masara","textLeft":"that while marriage isn't everything,"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":302901,"motion":0,"pos":0}],"nameLeft":"Masara","textLeft":"it's something that many people do@because they want to be happy."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":301601,"motion":100,"pos":2}],"nameRight":"Kokoro","textRight":"Yeah, you're right about that."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"Happiness is different for everyone, and@each person has their own ideas about family."},{"chara":[{"id":301601,"motion":0,"pos":2}],"nameRight":"Kokoro","textRight":"I honestly believe that getting married@isn't always the right answer."},{"chara":[{"id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"But, people who want happiness...@Like all the people here..."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"They're all smiling."},{"chara":[{"id":302901,"motion":400,"pos":0}],"nameLeft":"Masara","textLeft":"...Indeed, they are."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":301601,"motion":400,"pos":2}],"nameRight":"Kokoro","textRight":"Then, maybe that's it..."},{"chara":[{"id":301601,"pos":2}],"nameRight":"Kokoro","textRight":"The thing to keep in mind while modeling..."},{"chara":[{"id":302901,"motion":200,"pos":0}],"nameLeft":"Masara","textLeft":"...is to smile, as it turns out."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":301601,"motion":100,"pos":2}],"nameRight":"Kokoro","textRight":"That's it―! They say good fortune comes@to the homes of those who smile!"},{"chara":[{"id":302901,"motion":100,"pos":0}],"nameLeft":"Masara","textLeft":"*Chuckle*@That's true..."},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn"}]},"version":3}