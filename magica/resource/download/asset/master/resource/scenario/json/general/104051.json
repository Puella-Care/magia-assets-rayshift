{"story":{"group_1":[{"autoTurnFirst":3.8,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_01"}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":104000}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":1}]},{"autoTurnFirst":8.5,"chara":[{"id":104000,"motion":0}]},{"autoTurnFirst":3.4,"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":400}]},{"autoTurnFirst":5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":100}]}],"group_10":[{"autoTurnFirst":5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":1,"pos":1,"voice":"vo_char_1040_00_17"}]},{"autoTurnFirst":4.5,"chara":[{"id":104000,"motion":0}]},{"autoTurnFirst":2.7,"chara":[{"id":104000,"motion":100}]}],"group_11":[{"autoTurnFirst":4.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":300,"pos":1,"voice":"vo_char_1040_00_18"}]},{"autoTurnFirst":2.3,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":0}]},{"autoTurnFirst":5,"chara":[{"eyeClose":1,"id":104000,"motion":100}]}],"group_12":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_19"}]}],"group_13":[{"autoTurnFirst":5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_051.exp3.json","id":104000,"motion":200,"pos":1,"voice":"vo_char_1040_00_20"}]}],"group_14":[{"autoTurnFirst":2.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_051.exp3.json","id":104000,"motion":0,"pos":1,"voice":"vo_char_1040_00_21"}]},{"autoTurnFirst":2.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104000}]},{"autoTurnFirst":2.7,"chara":[{"id":104000,"motion":100}]},{"autoTurnFirst":4.5,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_000.exp3.json","id":104000}]}],"group_15":[{"autoTurnFirst":3.7,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":1,"pos":1,"voice":"vo_char_1040_00_22"}]},{"autoTurnFirst":4.5,"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":104000,"motion":300}]},{"autoTurnFirst":5.8,"chara":[{"id":104000,"motion":100}]},{"autoTurnFirst":3.2,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_000.exp3.json","id":104000,"motion":0}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":104000}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":1}]}],"group_16":[{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_051.exp3.json","id":104000,"motion":300,"pos":1,"voice":"vo_char_1040_00_23"}]},{"autoTurnFirst":3.9,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp3.json","id":104000}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":104000}]},{"autoTurnFirst":6,"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":400}]},{"autoTurnFirst":5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":100}]}],"group_17":[{"autoTurnFirst":2.6,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":400,"pos":1,"voice":"vo_char_1040_51_24","textHome":"Merry Christmas."}]},{"autoTurnFirst":6.7,"chara":[{"id":104051,"motion":1,"textHome":"In honor of the season, this outfit was based on@one of the three Wise Men of the East."}]},{"autoTurnFirst":5,"chara":[{"eyeClose":1,"id":104051,"motion":300,"textHome":"The decision was made@at Sensei’s discretion, of course."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_18":[{"autoTurnFirst":4.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":1,"pos":1,"voice":"vo_char_1040_51_25","textHome":"It truly is cold in the morning, isn’t it…?"}]},{"autoTurnFirst":6.2,"chara":[{"cheek":0,"face":"mtn_ex_051.exp3.json","id":104051,"textHome":"Speaking of which, it seems@you forgot your gloves.@Are you cold?"}]},{"autoTurnFirst":6,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":100,"textHome":"If you’d like, we could walk@hand in hand for the rest of the day."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_19":[{"autoTurnFirst":1.7,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_051.exp3.json","id":104051,"motion":300,"pos":1,"voice":"vo_char_1040_51_26","textHome":"If you are on a diet,@how about chicken for lunch?"}]},{"autoTurnFirst":2.9,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051}]},{"autoTurnFirst":3.1,"chara":[{"id":104051,"motion":0,"textHome":"With the exception of the skin,@it is healthy and tasty."}]},{"autoTurnFirst":4.3,"chara":[{"id":104051,"motion":200,"textHome":"Shall I prepare a dish for you?"}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_2":[{"autoTurnFirst":3.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_02"}]},{"autoTurnFirst":3.3,"chara":[{"id":104000,"motion":1}]},{"autoTurnFirst":5.5,"chara":[{"id":104000,"motion":300}]}],"group_20":[{"autoTurnFirst":2.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":1,"pos":1,"voice":"vo_char_1040_51_27","textHome":"It is time for dinner."}]},{"autoTurnFirst":3.3,"chara":[{"eyeClose":1,"id":104051,"textHome":"Now, if I may be so bold, I will play the piano."}]},{"autoTurnFirst":0.4,"chara":[{"eyeClose":0,"id":104051}]},{"autoTurnFirst":4.2,"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":100,"textHome":"In the end, I never did master my lessons,"}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"textHome":"but I hope it will make for a@modest accompaniment to your meal."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_21":[{"autoTurnFirst":3.9,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":300,"pos":1,"voice":"vo_char_1040_51_28","textHome":"I’ve been handing out gifts@to the crowd with Sudachi all day,"}]},{"autoTurnFirst":2.8,"chara":[{"eyeClose":1,"id":104051,"textHome":"and I’m exhausted…"}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":104051,"textHome":"Let’s take a break in the Care Trailer."}]},{"autoTurnFirst":2.6,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":0}]},{"autoTurnFirst":3,"chara":[{"eyeClose":1,"id":104051,"textHome":"I feel most at home there."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_22":[{"autoTurnFirst":7.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":100,"pos":1,"voice":"vo_char_1040_51_29","textHome":"To be frank, I’m uncertain@if this three Wise Men outfit suits me..."}]},{"autoTurnFirst":2.9,"chara":[{"eyeClose":1,"id":104051,"motion":1,"textHome":"The models are all male."}]},{"autoTurnFirst":0.3,"chara":[{"eyeClose":0,"id":104051}]},{"autoTurnFirst":5,"chara":[{"id":104051,"motion":400,"textHome":"I am...not sure if I look good in men’s fashion..."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_23":[{"autoTurnFirst":2.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp3.json","id":104051,"motion":300,"pos":1,"voice":"vo_char_1040_51_30","textHome":"A Witch…?"}]},{"autoTurnFirst":2.5,"chara":[{"eyeClose":1,"id":104051,"textHome":"On a day like this..."}]},{"autoTurnFirst":0.3,"chara":[{"eyeClose":0,"id":104051,"textHome":"Such is the situation, I suppose.@Do your best to defeat it."}]},{"autoTurnFirst":4.6,"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":1}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":100,"textHome":"If you win,@the Three Wise Men will give you a gift."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_24":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104051,"motion":0,"pos":1,"voice":"vo_char_1040_51_31","textHome":"Now that I am one of the three Wise Men,@I should be able to solve@ the mystery of the Mirror Witch..."}]},{"autoTurnFirst":3,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":200,"textHome":"...or perhaps not."}]},{"autoTurnFirst":3.2,"chara":[{"id":104051,"motion":1,"textHome":"This is simply a costume."}]},{"autoTurnFirst":4.5,"chara":[{"eyeClose":1,"id":104051,"textHome":"Inside is the usual Sasame Yozuru."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_25":[{"autoTurnFirst":4.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104051,"motion":400,"pos":1,"voice":"vo_char_1040_51_32"}]},{"autoTurnFirst":4.1,"chara":[{"id":104051,"motion":200}]},{"autoTurnFirst":2.4,"chara":[{"cheek":0,"face":"mtn_ex_051.exp3.json","id":104051,"motion":0}]},{"autoTurnFirst":7,"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":300}]}],"group_26":[{"autoTurnFirst":5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104051,"motion":100,"pos":1,"voice":"vo_char_1040_51_33","textHome":"As a Wise Man,@I put on glasses to appear more scholarly."}]},{"autoTurnFirst":2.8,"chara":[{"cheek":0,"face":"mtn_ex_051.exp3.json","id":104051,"textHome":"How do you like it…?"}]},{"autoTurnFirst":3.8,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":1,"textHome":"I believe I’ve changed my image a bit,"}]},{"autoTurnFirst":4,"chara":[{"id":104051,"motion":400,"textHome":"but the evaluation of a third party@is what matters most."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_27":[{"autoTurnFirst":3.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_051.exp3.json","id":104051,"motion":0,"pos":1,"voice":"vo_char_1040_51_34","textHome":"Perhaps because of the way I’m dressed,"}]},{"autoTurnFirst":6.7,"chara":[{"id":104051,"motion":100,"textHome":"whenever I went out into town,@people would approach me to ask for photos."}]},{"autoTurnFirst":4.1,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp3.json","id":104051,"motion":300,"textHome":"I don’t dislike it, though."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_28":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":1,"pos":1,"voice":"vo_char_1040_51_35","textHome":"I made a small house out of sweets for a party."}]},{"autoTurnFirst":2.5,"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":104051,"motion":300,"textHome":"It turned out very well,@but I made it too elaborate,@so no one wanted to touch it."}]},{"autoTurnFirst":4.3,"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":104051}]},{"autoTurnFirst":3.6,"chara":[{"eyeClose":1,"id":104051,"motion":0,"textHome":"Another mistake..."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_29":[{"autoTurnFirst":3.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_051.exp3.json","id":104051,"motion":0,"pos":1,"voice":"vo_char_1040_51_36","textHome":"Are you interested in this film?"}]},{"autoTurnFirst":3,"chara":[{"id":104051,"motion":100,"textHome":" It’s a romance movie, I believe…?"}]},{"autoTurnFirst":2.8,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"textHome":"It seems fitting for Christmas."}]},{"autoTurnFirst":4,"chara":[{"id":104051,"motion":400,"textHome":"If you’d like, I will watch it with you."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_3":[{"autoTurnFirst":2.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_03"}]},{"autoTurnFirst":3,"chara":[{"id":104000,"motion":400}]}],"group_30":[{"autoTurnFirst":7.1,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104051,"motion":100,"pos":1,"voice":"vo_char_1040_51_37","textHome":"I would like to prepare a gift for Sudachi.@Could you help me choose something…?"}]},{"autoTurnFirst":6.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":1,"textHome":"It is a kindness to somehow@repay your benefactors."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_31":[{"autoTurnFirst":5.1,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104051,"motion":1,"pos":1,"voice":"vo_char_1040_51_38","textHome":"Sensei decorated our Care Trailer@with lots of lights,"}]},{"autoTurnFirst":3.7,"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":300,"textHome":"but then the battery died@and caused quite the commotion."}]},{"autoTurnFirst":4,"chara":[{"eyeClose":1,"id":104051,"textHome":"I wish she’d be a little more prudent."}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_32":[{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp3.json","id":104051,"motion":1,"pos":1,"voice":"vo_char_1040_51_39","textHome":"If I am considered a Wise Man,@then that would make Sensei@a bad person as well."}]},{"autoTurnFirst":4.5,"chara":[{"eyeClose":0,"id":104051,"textHome":"There is no greater fool than myself."}]},{"autoTurnFirst":4.5,"chara":[{"id":104051,"motion":100,"textHome":"If only I had a little more wisdom@to control myself..."}]},{"autoTurnFirst":5,"chara":[{"cheek":0,"face":"mtn_ex_051.exp3.json","id":104051,"motion":400,"textHome":"Ah, forget it! We should be enjoying ourselves!"}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_33":[{"autoTurnFirst":5.6,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":100,"pos":1,"voice":"vo_char_1040_51_40","textHome":"When I was little, my mother@bought me a toy piano for Christmas."}]},{"autoTurnFirst":4.8,"chara":[{"eyeClose":1,"id":104051,"motion":300,"textHome":"I loved it and played with it like crazy."}]},{"autoTurnFirst":0.3,"chara":[{"eyeClose":0,"id":104051,"textHome":"It’s a fond memory."}]},{"autoTurnFirst":4,"chara":[{"id":104051,"motion":0}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_34":[{"autoTurnFirst":2.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp3.json","id":104051,"motion":0,"pos":1,"voice":"vo_char_1040_51_41","textHome":"Gutsy, aren’t you?"}]},{"autoTurnFirst":2.1,"chara":[{"id":104051,"motion":200,"textHome":"Hmph!"}]},{"autoTurnFirst":5.2,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104051,"motion":100,"textHome":"...Alright, I’ve gotten rid of the horsefly@that came into the trailer."}]},{"autoTurnFirst":3.7,"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":104051,"motion":1,"textHome":"It’s rather impressive to have that much energy@during Christmastime."}]},{"autoTurnFirst":3,"chara":[{"eyeClose":1,"id":104051}]},{"chara":[{"id":104051,"textHomeStatus":"Clear"}]}],"group_35":[{"autoTurnFirst":4.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp3.json","id":104000,"motion":400,"pos":1,"voice":"vo_char_1040_00_42"}]}],"group_36":[{"autoTurnFirst":3.9,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_030.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_43"}]}],"group_37":[{"autoTurnFirst":3.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":1,"pos":1,"voice":"vo_char_1040_00_44"}]}],"group_38":[{"autoTurnFirst":3.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":200,"pos":1,"voice":"vo_char_1040_00_45"}]}],"group_39":[{"autoTurnFirst":2.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp3.json","id":104000,"motion":200,"pos":1,"voice":"vo_char_1040_00_46"}]}],"group_4":[{"autoTurnFirst":4.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_030.exp3.json","id":104000,"motion":300,"pos":1,"voice":"vo_char_1040_00_04"}]}],"group_5":[{"autoTurnFirst":1.7,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_000.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_05"}]},{"autoTurnFirst":3.3,"chara":[{"eyeClose":0,"id":104000}]}],"group_6":[{"autoTurnFirst":4.1,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_040.exp3.json","id":104000,"motion":300,"pos":1,"voice":"vo_char_1040_00_13"}]}],"group_7":[{"autoTurnFirst":2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_14"}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"face":"mtn_ex_000.exp3.json","id":104000}]}],"group_8":[{"autoTurnFirst":5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_030.exp3.json","id":104000,"motion":1,"pos":1,"voice":"vo_char_1040_00_15"}]}],"group_9":[{"autoTurnFirst":2.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":104000,"motion":100,"pos":1,"voice":"vo_char_1040_00_16"}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp3.json","id":104000}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":104000}]},{"autoTurnFirst":5.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":104000,"motion":400}]}]},"version":3}
