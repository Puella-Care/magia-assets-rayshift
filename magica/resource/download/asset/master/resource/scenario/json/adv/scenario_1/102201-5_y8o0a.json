anagi","textCenter":"(Who is it...?!)"},{"chara":[{"id":101601,"lipSynch":0,"voiceFull":"fullvoice/section_102201/vo_full_102201-5-55","voiceFullStop":"fullvoice/section_102201/vo_full_102201-5-54"}],"textCenter":"..."},{"autoTurnLast":3,"chara":[{"eyeClose":1,"id":101601,"voiceFullStop":"fullvoice/section_102201/vo_full_102201-5-55"}],"flashEffect":"flashWhiteSlow2","se":"7211_souljem_02","textClear":1},{"chara":[{"eyeClose":0,"id":101601,"lipSynch":0,"motion":300,"voiceFull":"fullvoice/section_102201/vo_full_102201-5-56"}],"textCenter":"(I'd heard the reports, but there really is a@mysterious Magical Girl out there...)"},{"chara":[{"id":101601,"lipSynch":0,"voiceFull":"fullvoice/section_102201/vo_full_102201-5-57","voiceFullStop":"fullvoice/section_102201/vo_full_102201-5-56"}],"textCenter":"..."},{"chara":[{"id":101601,"lipSynch":1,"motion":0,"voiceFull":"fullvoice/section_102201/vo_full_102201-5-58","voiceFullStop":"fullvoice/section_102201/vo_full_102201-5-57"}],"textCenter":"Seems she disappeared..."},{"autoTurnLast":2,"bgm":"stop","turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_102201/vo_full_102201-5-58"}]},"version":3}
