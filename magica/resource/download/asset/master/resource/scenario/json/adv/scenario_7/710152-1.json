tion":0,"pos":2},{"cheek":0,"face":"mtn_ex_010.exp.json","id":801200,"motion":0,"pos":1}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":801000},{"effect":"fadeout","id":801100},{"effect":"fadeout","id":801200}]},{"autoTurnLast":1.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":801300,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":801400,"motion":0,"pos":2},{"cheek":0,"face":"mtn_ex_010.exp.json","id":801500,"motion":0,"pos":1}]},{"autoTurnLast":0.3,"chara":[{"effect":"fadeout","id":801300},{"effect":"fadeout","id":801400},{"effect":"fadeout","id":801500}]},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":101551,"motion":100,"pos":0},{"cheek":0,"face":"mtn_ex_050.exp.json","id":100153,"motion":0,"pos":2}],"nameLeft":"Ui","textLeft":"I didn't think...there would be so many people..."},{"chara":[{"id":100153,"motion":301,"pos":2}],"nameRight":"Iroha","textRight":"...But you're not bad with handling crowds...?"},{"chara":[{"id":101551,"motion":1}],"nameLeft":"Ui","textLeft":"Yeah, but...this haregi..."},{"chara":[{"id":100153,"motion":0,"pos":2}],"nameRight":"Iroha","textRight":"Ah..."},{"chara":[{"id":101551}],"nameLeft":"Ui","textLeft":"Because Mom sent it to me..."},{"chara":[{"id":101551,"motion":300}],"nameLeft":"Ui","textLeft":"I don't want it to get dirty or wrinkled..."},{"chara":[{"id":100153}],"nameRight":"Iroha","textRight":"I see...that's why..."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100153,"motion":100,"pos":2}],"nameRight":"Iroha","textRight":"But, there's no need to worry about that."},{"chara":[{"id":100153}],"nameRight":"Iroha","textRight":"I mean, didn't you already talk to Mom@on the phone?"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":101551,"motion":0,"pos":0}],"nameLeft":"Ui","textLeft":"The phone...?@Before New Year's...?"},{"autoTurnLast":1,"bg":"bg_adv_narration.jpg","chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":101502,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":800600,"motion":0,"pos":2}],"turnChangeIn":"fadeIn","turnEffect":"thinking"},{"chara":[{"id":101502,"motion":100,"pos":0}],"nameLeft":"Ui","textLeft":"Mom, the haregi arrived.@Thank you!","turnChangeOut":"fadeOut"},{"chara":[{"id":800600}],"nameRight":"Ui's Mother","textRight":"Does the size fit you?"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":101502,"pos":0}],"nameLeft":"Ui","textLeft":"Yeah, perfectly!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":800600,"pos":2}],"nameRight":"Ui's Mother","textRight":"That's great."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":800600,"pos":2},{"cheek":0,"face":"mtn_ex_010.exp.json","id":101502,"motion":0}],"nameRight":"Ui's Mother","textRight":"It's easy to move in and warm,@so enjoy yourself this New Year."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":101502,"motion":400,"pos":0}],"nameLeft":"Ui","textLeft":"Yeah!"},{"autoTurnLast":1,"bg":"bg_adv_21401.jpg","bgm":"stop","chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100153,"motion":0,"pos":2},{"cheek":0,"face":"mtn_ex_050.exp.json","id":101551,"motion":0,"pos":0}],"turnChangeIn":"fadeIn","turnEffect":"stop"},{"bgm":"bgm03_story09","chara":[{"id":100153,"motion":200,"pos":2}],"nameRight":"Iroha","textRight":"Mom told you to \"enjoy yourself,\" right?","turnChangeOut":"fadeOut"},{"chara":[{"id":101551,"motion":100}],"nameLeft":"Ui","textLeft":"Ah, that's right...!"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":101551,"pos":0}],"nameLeft":"Ui","textLeft":"She picked one out that was easy to move in@so I could enjoy it."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":101551,"motion":200,"pos":0}],"nameLeft":"Ui","textLeft":"Got it!@I'll do lots of shopping then!"},{"chara":[{"id":100153,"motion":100}],"nameRight":"Iroha","textRight":"*Giggle* That's good."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100153,"pos":2},{"cheek":0,"face":"mtn_ex_010.exp.json","id":101551,"motion":0}],"nameRight":"Iroha","textRight":"Well then, let's continue our shopping trip."},{"chara":[{"id":100153,"motion":401,"pos":2}],"nameRight":"Iroha","textRight":"What was it that Ui wanted?"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":101551,"motion":1,"pos":0}],"nameLeft":"Ui","textLeft":"Umm...there was a general store that had@cute accessories, a wallet, and..."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":101551,"motion":400,"pos":0}],"nameLeft":"Ui","textLeft":"[chara:101551:effect_emotion_joy_0][se:7222_happy]Ah...a while ago I saw a backpack I was interested in!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":101551,"pos":0}],"nameLeft":"Ui","textLeft":"Also also, sneakers too!@I want a pair that matches Sis!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100153,"motion":200,"pos":2}],"nameRight":"Iroha","textRight":"*Giggle*"},{"autoTurnLast":2,"turnChangeIn":"fadeIn"}]},"version":3}
