":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":833500,"motion":300,"pos":2}],"nameRight":"Sakie","textRight":"Oh my,@I'm happy to join in."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":833500}],"nameRight":"Sakie","textRight":"Mabayu isn't here, and I was getting@bored..."},{"chara":[{"id":833500}],"nameRight":"Sakie","textRight":"Maybe I’ll join the chatter"},{"chara":[{"id":833500,"motion":1}],"nameRight":"Sakie","textRight":"until the next customer arrives."},{"chara":[{"cheek":0,"face":"mtn_ex_040.exp3.json","id":200301,"motion":0,"pos":0}],"nameLeft":"Homura","textLeft":"Mabayu…?"},{"chara":[{"id":833500}],"nameRight":"Saki","textRight":"Ah, she usually helps out here."},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn"}]},"version":3}
