d":402400,"motion":300}]}],"group_15":[{"autoTurnFirst":1.8,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_22"}]},{"autoTurnFirst":5.8,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp.json","id":402400}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":402400}]},{"autoTurnFirst":5.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402400,"motion":300}]},{"autoTurnFirst":7.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":100}]}],"group_16":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_23"}]},{"autoTurnFirst":3.2,"chara":[{"eyeClose":1,"id":402400}]},{"autoTurnFirst":2.0,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_011.exp.json","id":402400,"motion":200}]},{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp.json","id":402400}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":402400}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402400}]}],"group_17":[{"autoTurnFirst":4.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":1,"pos":1,"voice":"vo_char_4024_50_24","textHome":"Good day.@I see you here rather often."}]},{"autoTurnFirst":5.3,"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":402450,"textHome":"Are you interested in@joining the student council?"}]},{"autoTurnFirst":6.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":100,"textHome":"If you swear absolute obedience to us,@I may consider it."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_18":[{"autoTurnFirst":3.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":0,"pos":1,"voice":"vo_char_4024_50_25","textHome":"Ah, you're awake."}]},{"autoTurnFirst":3.7,"chara":[{"id":402450,"motion":300,"textHome":"Would you care for a cup of tea@to get rid of your drowsiness?"}]},{"autoTurnFirst":3.6,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_011.exp.json","id":402450,"motion":100,"textHome":"*Chuckle*@It does smell good, doesn't it?"}]},{"autoTurnFirst":1.3,"chara":[{"id":402450,"motion":1,"textHome":"Not to worry,@there's certainly nothing strange in it."}]},{"autoTurnFirst":5.0,"chara":[{"eyeClose":0,"id":402450}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_19":[{"autoTurnFirst":4.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":200,"pos":1,"voice":"vo_char_4024_50_26","textHome":"Would you care to join us for lunch@in the student council room?"}]},{"autoTurnFirst":2.3,"chara":[{"id":402450,"motion":1,"textHome":"If you come with me,@there's no guarantee you'll come back out."}]},{"autoTurnFirst":2.8,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402450}]},{"autoTurnFirst":1.1,"chara":[{"eyeClose":1,"id":402450,"motion":100,"textHome":"*Chuckle*@I'm just kidding."}]},{"autoTurnFirst":3.0,"chara":[{"eyeClose":0,"id":402450}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_2":[{"autoTurnFirst":6.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_02"}]},{"autoTurnFirst":3.0,"chara":[{"id":402400,"motion":200}]},{"autoTurnFirst":3.1,"chara":[{"id":402400,"motion":100}]},{"autoTurnFirst":3.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402400}]}],"group_20":[{"autoTurnFirst":3.1,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":400,"pos":1,"voice":"vo_char_4024_50_27","textHome":"Oh, is it this late already?"}]},{"autoTurnFirst":3.0,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":402450,"motion":0,"textHome":"My my, I had better call home soon."}]},{"autoTurnFirst":5.5,"chara":[{"id":402450,"motion":300,"textHome":"My anxious sisters may@report me missing otherwise."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_21":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":1,"pos":1,"voice":"vo_char_4024_50_28","textHome":"The clock is ticking.@The time when everything shall be@cloaked in darkness..."}]},{"autoTurnFirst":2.1,"chara":[{"eyeClose":1,"id":402450}]},{"autoTurnFirst":0.1,"chara":[{"eyeClose":0,"id":402450}]},{"autoTurnFirst":3.2,"chara":[{"cheek":2,"face":"mtn_ex_030.exp.json","id":402450,"motion":300,"textHome":"Ah, did I frighten you...?"}]},{"autoTurnFirst":6.0,"chara":[{"eyeClose":1,"id":402450,"motion":0,"textHome":"Pardon, I'm simply at the edge of exhaustion."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_22":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp.json","id":402450,"motion":100,"pos":1,"voice":"vo_char_4024_50_29","textHome":"A cup of English tea@is truly the best way to relax."}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":402450}]},{"autoTurnFirst":3.7,"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":0,"textHome":"Caf√© au lait seems to be popular@among the students, but what's so good@about that muddy drink...?"}]},{"autoTurnFirst":5.0,"chara":[{"id":402450,"motion":200}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_23":[{"autoTurnFirst":3.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp.json","id":402450,"motion":200,"pos":1,"voice":"vo_char_4024_50_30","textHome":"There have been some disturbing signs lately."}]},{"autoTurnFirst":5.3,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":402450,"motion":1,"textHome":"I don't care for physical work, so please@don't make me do something unsightly@like scare away ghosts..."}]},{"autoTurnFirst":4.0,"chara":[{"eyeClose":1,"id":402450,"motion":300}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_24":[{"autoTurnFirst":6.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":200,"pos":1,"voice":"vo_char_4024_50_31","textHome":"Some people have reported seeing@a copy of me in a certain mirrored hall..."}]},{"autoTurnFirst":6.5,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":402450,"motion":100,"textHome":"It's bad enough that the only student@who looks like me is Tart..."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_25":[{"autoTurnFirst":4.7,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":1,"pos":1,"voice":"vo_char_4024_50_32"}]},{"autoTurnFirst":2.7,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":0}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":300}]}],"group_26":[{"autoTurnFirst":3.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":0,"pos":1,"voice":"vo_char_4024_50_33","textHome":"Are you curious about this uniform?"}]},{"autoTurnFirst":3.7,"chara":[{"eyeClose":1,"id":402450,"motion":300,"textHome":"It's specially made for the student council."}]},{"autoTurnFirst":0.1,"chara":[{"eyeClose":0,"id":402450}]},{"autoTurnFirst":1.7,"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":1,"textHome":"For us, there is no such thing@as breaking school rules!"}]},{"autoTurnFirst":2.0,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":402450}]},{"autoTurnFirst":4.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":200,"textHome":"We, the council, make the rules."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_27":[{"autoTurnFirst":3.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":400,"pos":1,"voice":"vo_char_4024_50_34","textHome":"Oh, where did you run off to?"}]},{"autoTurnFirst":3.2,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":402450,"motion":100,"textHome":"Truth be told, my vision behind this mask@is quite poor, so I can't see much@past the inside of it."}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":0}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_28":[{"autoTurnFirst":6.1,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":100,"pos":1,"voice":"vo_char_4024_50_35","textHome":"*Chuckle* There are some who would@mark the student council as cowardly,@but that is simply not true."}]},{"autoTurnFirst":2.1,"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":402450,"motion":0}]},{"autoTurnFirst":3.2,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":1,"textHome":"We are proud to hide our true faces while@denying anyone access to our council room."}]},{"autoTurnFirst":5.0,"chara":[{"id":402450,"motion":200}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_29":[{"autoTurnFirst":6.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":0,"pos":1,"voice":"vo_char_4024_50_36","textHome":"There's a rumor going around that@no one has ever seen me in a classroom..."}]},{"autoTurnFirst":2.9,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":200,"textHome":"However, my duties on the student council@keep me very busy."}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402450,"motion":300,"textHome":"I have no time for silly classes."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_3":[{"autoTurnFirst":4.0,"chara":[{"cheek":2,"eyeClose":1,"face":"mtn_ex_011.exp.json","id":402400,"motion":100,"pos":1,"voice":"vo_char_4024_00_03"}]}],"group_30":[{"autoTurnFirst":7.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402450,"motion":1,"pos":1,"voice":"vo_char_4024_50_37","textHome":"The previous student council president@was Lapin; currently it is Corbeau."}]},{"autoTurnFirst":5.0,"chara":[{"id":402450,"motion":100,"textHome":"I'm the vice president,@but I do most of the work."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_31":[{"autoTurnFirst":5.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_030.exp.json","id":402450,"motion":300,"pos":1,"voice":"vo_char_4024_50_38","textHome":"Lapin has pink hair,@while Corbeau has silver hair."}]},{"autoTurnFirst":3.7,"chara":[{"id":402450,"motion":1,"textHome":"Some people say they don't look alike@although they're sisters,@but that is clearly false."}]},{"autoTurnFirst":2.2,"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":402450}]},{"autoTurnFirst":5.0,"chara":[{"id":402450,"motion":200,"textHome":"The similarities are obvious."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_32":[{"autoTurnFirst":2.8,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp.json","id":402450,"motion":0,"pos":1,"voice":"vo_char_4024_50_39","textHome":"Is it because of his family connection@that he gained his position?"}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":402450}]},{"autoTurnFirst":5.8,"chara":[{"id":402450,"motion":300,"textHome":"I can't help but notice that the Principal@is behaving a little more confidently..."}]},{"autoTurnFirst":6.0,"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":402450,"motion":200,"textHome":"I'll have to speak to@Vice Principal La Tremoille about this."}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_33":[{"autoTurnFirst":3.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp.json","id":402450,"motion":200,"pos":1,"voice":"vo_char_4024_50_40","textHome":"Oh, you seem tired."}]},{"autoTurnFirst":3.3,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":402450,"motion":1,"textHome":"As they say, study is the essence@of the student, but sometimes@you do need to take a break."}]},{"autoTurnFirst":3.4,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402450}]},{"autoTurnFirst":2.9,"chara":[{"id":402450,"motion":100,"textHome":"I know of a nice hot springs nearby;@I'll invite you next time."}]},{"autoTurnFirst":4.0,"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":402450}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_34":[{"autoTurnFirst":2.7,"chara":[{"cheek":2,"eyeClose":1,"face":"mtn_ex_011.exp.json","id":402450,"motion":300,"pos":1,"voice":"vo_char_4024_50_41","textHome":"Haaah...!@Mother...!"}]},{"autoTurnFirst":1.8,"chara":[{"cheek":2,"eyeClose":1,"face":"mtn_ex_030.exp.json","id":402450,"textHome":"Her form is like unto a Goddess~...!"}]},{"autoTurnFirst":2.3,"chara":[{"cheek":2,"eyeClose":1,"face":"mtn_ex_010.exp.json","id":402450,"motion":1}]},{"autoTurnFirst":0.3,"chara":[{"eyeClose":0,"id":402450}]},{"autoTurnFirst":3.9,"chara":[{"cheek":1,"face":"mtn_ex_050.exp.json","id":402450,"motion":400,"textHome":"...*Gasp*?!@How long have you been there?!"}]},{"autoTurnFirst":1.9,"chara":[{"cheek":2,"face":"mtn_ex_020.exp.json","id":402450,"motion":0,"textHome":"I-I was just...@looking at the wallpaper on my phone!"}]},{"autoTurnFirst":4.0,"chara":[{"eyeClose":1,"id":402450,"motion":200}]},{"chara":[{"id":402450,"textHomeStatus":"Clear"}]}],"group_35":[{"autoTurnFirst":5.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp.json","id":402400,"motion":200,"pos":1,"voice":"vo_char_4024_00_42"}]}],"group_36":[{"autoTurnFirst":5.0,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_43"}]}],"group_37":[{"autoTurnFirst":2.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":100,"pos":1,"voice":"vo_char_4024_00_44"}]},{"autoTurnFirst":3.0,"chara":[{"eyeClose":1,"id":402400}]}],"group_38":[{"autoTurnFirst":5.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":200,"pos":1,"voice":"vo_char_4024_00_45"}]}],"group_39":[{"autoTurnFirst":5.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_46"}]}],"group_4":[{"autoTurnFirst":4.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":200,"pos":1,"voice":"vo_char_4024_00_04"}]}],"group_5":[{"autoTurnFirst":5.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_05"}]}],"group_6":[{"autoTurnFirst":4.0,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_050.exp.json","id":402400,"motion":200,"pos":1,"voice":"vo_char_4024_00_13"}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":402400}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402400}]}],"group_7":[{"autoTurnFirst":3.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":402400,"motion":1,"pos":1,"voice":"vo_char_4024_00_14"}]},{"autoTurnFirst":5.0,"chara":[{"eyeClose":1,"id":402400}]}],"group_8":[{"autoTurnFirst":7.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":200,"pos":1,"voice":"vo_char_4024_00_15"}]}],"group_9":[{"autoTurnFirst":2.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":402400,"motion":300,"pos":1,"voice":"vo_char_4024_00_16"}]},{"autoTurnFirst":4.0,"chara":[{"eyeClose":1,"id":402400}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":402400}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":402400,"motion":1}]}]},"version":3}
