:[{"id":304501}],"nameRight":"Rion","textRight":"We need to go shopping together this weekend..."},{"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":304501,"motion":400}],"nameRight":"Rion","textRight":"I need to relieve some stress!"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":304101,"motion":0,"pos":0}],"nameLeft":"Hotori","textLeft":"Y-yeah... Let's go together."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":304501}],"nameRight":"Rion","textRight":"We'll buy clothes, accessories, tea..."},{"chara":[{"id":304501,"motion":1}],"nameRight":"Rion","textRight":"...And, if we have any time left over..."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":304501,"pos":2}],"nameRight":"Rion","textRight":"Then we can go to whatever stores you want."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":304101,"motion":400,"pos":0}],"nameLeft":"Hotori","textLeft":"A-are you sure?!"},{"chara":[{"id":304501,"motion":100}],"nameRight":"Rion","textRight":"If there's time left over."},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":304101,"motion":100,"pos":0}],"nameLeft":"Hotori","textLeft":"Y-yay! I'm looking forward to it...!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":304501,"motion":200,"pos":2}],"nameRight":"Rion","textRight":"...*Giggle* Good."},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn"}]},"version":3}
