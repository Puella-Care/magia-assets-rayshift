{"story":{"group_1":[{"autoTurnLast":0.01,"bg":"bg_adv_11092.jpg","bgm":"bgm01_anime12","chara":[{"cheek":0,"face":"mtn_ex_001.exp3.json","id":200500,"motion":0,"pos":1}],"turnChangeIn":"fadeIn"},{"chara":[{"id":200500,"motion":200}],"nameCenter":"Mami","textCenter":"Is everybody ready?","turnChangeOut":"fadeOut"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":200500}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp3.json","id":200100,"motion":201,"mouthOpen":1,"pos":0},{"cheek":0,"face":"mtn_ex_020.exp3.json","id":200300,"motion":0,"mouthOpen":1,"pos":2}],"nameCenter":"Madoka & Homura","textCenter":"Yes!"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":200100},{"effect":"fadeout","id":200300}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp3.json","id":200400,"motion":201,"pos":0},{"cheek":0,"face":"mtn_ex_020.exp3.json","id":302500,"motion":0,"pos":2}],"nameLeft":"Sayaka","textLeft":"Leave it to me!"},{"chara":[{"id":302500,"motion":200}],"nameRight":"Ren","textRight":"...I'll do my best...!"},{"autoTurnLast":0.2,"chara":[{"effect":"fadeout","id":302500},{"effect":"fadeout","id":200400}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp3.json","id":303050,"motion":1,"pos":2},{"cheek":0,"face":"mtn_ex_030.exp3.json","id":100151,"motion":0,"pos":0}],"nameRight":"Konomi","textRight":"Will just the five of you really be enough?"},{"chara":[{"id":100151,"motion":100}],"nameLeft":"Iroha","textLeft":"If you feel like you're in danger,@please retreat immediately."},{"chara":[{"cheek":0,"face":"mtn_ex_021.exp3.json","id":100151}],"nameLeft":"Iroha","textLeft":"We'll be right here waiting for you!"},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp3.json","id":101552,"motion":200,"pos":2}],"nameRight":"Ui","textRight":"And if things get really bad,"},{"chara":[{"id":101552}],"nameRight":"Ui","textRight":"Yachiyo and the others are on their way, too!"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":200500,"motion":0,"pos":0}],"