a":[{"eyeClose":1,"id":303400}]}],"group_17":[{"autoTurnFirst":3.5,"chara":[{"cheek":1,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":303400,"motion":100,"pos":1,"voice":"vo_char_3034_00_24","textHome":"Wow, nice to see you again!@Would you like to chat?"}]},{"autoTurnFirst":2.0,"chara":[{"cheek":1,"face":"mtn_ex_010.exp.json","id":303400,"textHome":"Wow, nice to see you again!@Would you like to chat?"}]},{"autoTurnFirst":1.5,"chara":[{"cheek":1,"face":"mtn_ex_011.exp.json","id":303400,"textHome":"Wow, nice to see you again!@Would you like to chat?"}]},{"autoTurnFirst":5.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":303400,"motion":1,"textHome":"Oh, that reminds me,@I just found a nice café.@Want to...go with me?"}]},{"autoTurnFirst":2.0,"chara":[{"id":303400,"motion":200,"textHome":"Oh, that reminds me,@I just found a nice café.@Want to...go with me?"}]},{"chara":[{"id":303400,"textHomeStatus":"Clear"}]}],"group_18":[{"autoTurnFirst":7.5,"chara":[{"cheek":1,"eyeClose":1,"face":"mtn_ex_030.exp.json","id":303400,"motion":1,"pos":1,"voice":"vo_char_3034_00_25","textHome":"*Yawn* Mornin'...@I'm still sleepy..."}]},{"autoTurnFirst":3.7,"chara":[{"cheek":1,"eyeClose":1,"face":"mtn_ex_010.exp.json","id":303400,"motion":300,"textHome":"I've already given up@on getting out of bed…"}]},{"autoTurnFirst":0.5,"chara":[{"eyeClose":0,"id":303400,"textHome":"I've already given up@on getting out of bed…"}]},{"autoTurnFirst":1.5,"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":303400,"motion":400,"textHome":"Ah! Changed my mind!@A shop I know is selling@egg pudding today!"}]},{"autoTurnFirst":3.8,"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":303400,"motion":100,"textHome":"Ah! Changed my mind!@A shop I know is selling@egg pudding today!"}]},{"chara":[{"id":303400,"textHomeStatus":"Clear"}]}],"group_19":[{"autoTurnFirst":3.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":303400,"motion":0,"pos":1,"voice":"vo_char_3034_00_26","textHome":"It's our long-awaited lunchtime!"}]},{"autoTurnFirst":4.0,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":303400,"motion":400,"textHome":"Huh? Did you forget your lunch?"}]},{"autoTurnFirst":5.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":303400,"motion":1,"textHome":"If you did, why don't I share@some of mine with you?@Here, say \"Aaaaah!\""}]},{"autoTurnFirst":3.5,"chara":[{"id":303400,"motion":200,"textHome":"If you did, why don't I share@some of mine with you?@Here, say \"Aaaaah!\""}]},{"chara":[{"id":303400,"textHomeStatus":"Clear"}]}],"group_2":[{"autoTurnFirst":3.4,"chara":[{"cheek":1,"eyeClose":0,"face":"mtn_ex_011.exp.json","id":303400,"motion":0,"pos":1,"voice":"vo_char_3034_00_02","textHome":"Nice to meet you!@I'm Moka Megumi."}]},{"autoTurnFirst":2.3,"chara":[{"cheek":1,"face":"mtn_ex_010.exp.json","id":303400,"motion":1,"textHome":"I'll do my best not@to drag you all down."}]},{"autoTurnFirst":2.5,"chara":[{"cheek":1,"face":"mtn_ex_020.exp.json","id":303400,"motion":100,"textHome":"I'll do my best not@to drag you all down."}]},{"autoTurnFirst":2.8,"chara":[{"cheek":1,"face":"mtn_ex_030.exp.json","id":303400,"motion":400,"textHome":"I won't cause any trouble."}]},{"chara":[{"id":303400,"textHomeStatus":"Clear"}]}],"group_20":[{"autoTurnFirst":4.0,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp.json","id":303400,"motion":200,"pos":1,"voice":"vo_char_3034_00_27","textHome":"The day is almost over."}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":303400,"motion":1,"textHome":"Seems I only messed@up four times today.@Mm-hm. Well done, me!"}]},{"autoTurnFirst":1.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":303400,"motion":100,"textHome":"Seems I only messed@up four times today.@Mm-hm. Well done, me!"}]},{"autoTurnFirst":2.5,"chara":[{"eyeClose":1,"id":303400,"textHome":"Seems I only messed@up four times today.@Mm-hm. Well done, me!"}]},{"chara":[{"id":303400,"textHomeStatus":"Clear"}]}],"group_21":[